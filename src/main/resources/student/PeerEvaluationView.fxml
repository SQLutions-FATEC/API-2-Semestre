<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.PeerEvaluationController">
   <AnchorPane prefHeight="400.0" prefWidth="600.0">
      <children>
           <Label layoutX="200.0" layoutY="12.0" text="Consultar avaliações anteriores" />
           <ComboBox fx:id="previousEvaluationsComboBox" layoutX="450.0" layoutY="12.0" promptText="Selecione" />
      
          <TableView fx:id="evaluationTable" layoutY="50.0" prefHeight="300.0" prefWidth="600.0" />
      
          <Button fx:id="backButton" layoutX="200.0" layoutY="362.0" text="Voltar para tela de avaliação" />
      </children>
   </AnchorPane>
</VBox>
