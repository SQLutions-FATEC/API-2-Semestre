<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.StudentController">
   <children>
      <Label alignment="CENTER" layoutX="175.0" layoutY="25.0" text="Avaliação da Equipe" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" editable="true" layoutX="50.0" layoutY="150.0" prefHeight="325.0" prefWidth="700.0" />
      <Label fx:id="pointsInfo" alignment="CENTER" contentDisplay="CENTER" layoutX="300.0" layoutY="475.0" text="Pontos destinados: x (Limite: y)" />
      <Button layoutX="50.0" layoutY="525.0" mnemonicParsing="false" onAction="#goToLoginScreen" prefHeight="38.0" prefWidth="143.0" text="Deslogar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="sendButton" layoutX="605.0" layoutY="525.0" mnemonicParsing="false" onAction="#ConfirmarNotas" prefHeight="38.0" prefWidth="143.0" text="Enviar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="305.0" layoutY="100.0" mnemonicParsing="false" onAction="#goToPastEvaluationsScreen" prefHeight="30.0" prefWidth="198.0" text="Ver avaliaçoes passadas">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
